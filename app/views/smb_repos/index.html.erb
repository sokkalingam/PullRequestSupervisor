<h1>Listing smb_repos</h1>

<table>
  <thead>
    <tr>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @smb_repos.each do |smb_repo| %>
      <tr>
        <td><%= smb_repo.url %></td>
        <td><%= link_to 'Show', smb_repo %></td>
        <td><%= link_to 'Edit', edit_smb_repo_path(smb_repo) %></td>
        <td><%= link_to 'Destroy', smb_repo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Smb repo', new_smb_repo_path %>
