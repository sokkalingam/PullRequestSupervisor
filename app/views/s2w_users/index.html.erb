<h1>Listing s2w_users</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Display name</th>
      <th>Last commented</th>
      <th>Last merged</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @s2w_users.each do |s2w_user| %>
      <tr>
        <td><%= s2w_user.name %></td>
        <td><%= s2w_user.display_name %></td>
        <td><%= s2w_user.last_commented %></td>
        <td><%= s2w_user.last_merged %></td>
        <td><%= link_to 'Show', s2w_user %></td>
        <td><%= link_to 'Edit', edit_s2w_user_path(s2w_user) %></td>
        <td><%= link_to 'Destroy', s2w_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New S2w user', new_s2w_user_path %>
